{
  "name": "GithubCompareTool",
    "version": "0.2.0",
    "manifest_version": 2,
    "description": "Compareボタンを配置します",
    "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "js": [
              "jquery.min.js",
              "main.js"
            ],
      "run_at": "document_start"
    }
  ]
}
